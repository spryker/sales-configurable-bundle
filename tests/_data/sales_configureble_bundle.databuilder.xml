<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ConfiguredBundle">
        <property name="quantity" dataBuilderRule="=1" />
        <property name="groupKey" dataBuilderRule="unique()->text(10)" />
    </transfer>

    <transfer name="ConfigurableBundleTemplate">
        <property name="name" dataBuilderRule="unique()->sentence(2)" />
    </transfer>

</transfers>
